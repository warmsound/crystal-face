<fonts>
	<font id="HoursFont" filename="titillium-web-bold-110-tall.fnt" antialias="true"/>
	<font id="MinutesFont" filename="titillium-web-light-110-tall.fnt" antialias="true"/>
	<font id="SecondsFont" filename="titillium-web-semibold-39.fnt" antialias="true" filter="0123456789AP"/>
	<font id="DateFont" filename="titillium-web-semibold-39.fnt" antialias="true" filter=" 0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZÁÄÅÉÍÖØÚÛČĚŘŚŠŹŽ"/>

	<!-- Manually symlinked from resources-round-260x260/fonts. -->
	<font id="NormalFont" filename="../../resources-round-260x260/fonts/titillium-web-semibold-26.fnt" antialias="true" filter="%-.0123456789!:?CFabefgikmpsty°+PSon /"/>
	<font id="NormalFontCities" filename="../../resources-round-260x260/fonts/titillium-web-semibold-26.fnt" antialias="true" filter=" %',-.0123456789!:?ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz°ÅÍÎÖáãäåæèéíðñóôöøûüž+/"/>

	<font id="IconsFont" filename="crystal-icons-extra-large.fnt" antialias="true"/>

	<!-- Font size chosen such that height of sunshine icon is close to 39px (38px) -->
	<!-- .fnt file modified such that day icons start from "A", night icons start from "a", "broken clouds" ("I") is shared -->
	<font id="WeatherIconsFontDay" filename="weather-icons-39.fnt" antialias="true" filter="ABCDEFGHI"/>
	<font id="WeatherIconsFontNight" filename="weather-icons-39.fnt" antialias="true" filter="abcdefghI"/>

	<!-- Locale-specific manual date font overrides, because fonts cannot automatically be overridden by locale -->
	<font id="DateFontOverrideZHS" filename="noto-sans-cjk-sc-medium-39.fnt" antialias="true"/>
	<font id="DateFontOverrideZHT" filename="noto-sans-cjk-tc-medium-39.fnt" antialias="true"/>
	<font id="DateFontOverrideRUS" filename="noto-sans-rus-bold-39.fnt" antialias="true"/>
	<font id="DateFontOverrideKOR" filename="namum-gothic-kor-39.fnt" antialias="true"/>

	<!-- Always on fonts -->
	<font id="AlwaysOnHoursFont" filename="../../resources-round-280x280/fonts/titillium-web-light-80-tall.fnt" antialias="true"/>
	<font id="AlwaysOnMinutesFont" filename="../../resources-round-280x280/fonts/titillium-web-light-80-tall.fnt" antialias="true"/>
	<font id="AlwaysOnSecondsFont" filename="../../resources-round-280x280/fonts/titillium-web-semibold-28.fnt" antialias="true" filter="AP"/> <!-- AM/PM only -->
	<font id="AlwaysOnDateFont" filename="../../resources/fonts/titillium-web-semibold-24.fnt" antialias="true" filter=" 0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZÁÄÅÉÍÖØÚÛČĚŘŚŠŹŽ"/>
	<font id="AlwaysOnDateFontOverrideZHS" filename="../../resources/fonts/noto-sans-cjk-sc-medium-24.fnt" antialias="true"/>
	<font id="AlwaysOnDateFontOverrideZHT" filename="../../resources/fonts/noto-sans-cjk-tc-medium-24.fnt" antialias="true"/>
	<font id="AlwaysOnDateFontOverrideRUS" filename="../../resources/fonts/noto-sans-rus-bold-24.fnt" antialias="true"/>
	<font id="AlwaysOnDateFontOverrideKOR" filename="../../resources/fonts/namum-gothic-kor-24.fnt" antialias="true"/>
	<font id="AlwaysOnBatteryFont" filename="../../resources/fonts/titillium-web-semibold-24.fnt" antialias="true" filter="%0123456789Ts+-? "/>
</fonts>
